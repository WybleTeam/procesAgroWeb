{% extends 'ThemesBundle::backend.html.twig' %}

{% block body -%}
    <h1>Seccionales</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombreseccional</th>
                <td>{{ entity.nombreSeccional }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('seccionales') }}">
            Volver al listado
        </a>
    </li>
    <li>
        <a href="{{ path('seccionales_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
