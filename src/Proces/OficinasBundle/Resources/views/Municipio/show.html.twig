{% extends 'ThemesBundle::backend.html.twig' %}

{% block body -%}
    <h1>Municipio</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombremunicipio</th>
                <td>{{ entity.nombreMunicipio }}</td>
            </tr>
            <tr>
                <th>Tipomunicipio</th>
                <td>{{ entity.tipoMunicipio }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('municipio') }}">
           Volver al listado
        </a>
    </li>
    <li>
        <a href="{{ path('municipio_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
