{% extends 'ThemesBundle::backend.html.twig' %}
{% block body -%}
    <h1>Servicios</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Tituloservicio</th>
                <td>{{ entity.tituloServicio }}</td>
            </tr>
            <tr>
                <th>Descripcionservicio</th>
                <td>{{ entity.descripcionServicio }}</td>
            </tr>
            <tr>
                <th>Urlaudioservicio</th>
                <td><a href="{{ entity.urlAudioServicio }}" target="_blank">{{ entity.urlAudioServicio }}</a></td>
            </tr>
            <tr>
                <th>Urlservicio</th>
                <td><a href="{{ entity.urlServicio }}" target="_blank">{{ entity.urlServicio }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('servicios') }}">
            Volver al Listado
        </a>
    </li>
    <li>
        <a href="{{ path('servicios_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
